		<action name="op_*" class="action.HouseAction" method="{1}" >
		
			<result name="{1}_success" type="dispatcher">/jsp/house/{1}_success.jsp</result>
			<result name="{1}" type="dispatcher">/jsp/house/{1}.jsp</result>
			<result name="input" type="dispatcher">/jsp/house/input.jsp</result>
		</action>
		<action name="add" class="action.HouseAction" method="add">
			<result name="input" type="dispatcher">/jsp/house/add.jsp</result>
			<result name="add" type="dispatcher">/jsp/house/list.jsp</result>

		</action>
		<action name="fileupload" class="action.FileUploadAction">
			<result>/jsp/upload_success.jsp</result>
			<result name="error">/jsp/upload.jsp</result>
		
